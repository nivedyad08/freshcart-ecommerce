 <main>
  <!-- section -->
  <section>
    <div class="container">
      <!-- row -->
      <div class="row">
        <!-- col -->
        <div class="col-12">
          <div class="d-flex justify-content-between align-items-center d-md-none py-4">
            <!-- heading -->
            <h3 class="fs-5 mb-0">Account Setting</h3>
            <!-- button -->
            <button class="btn btn-outline-gray-400 text-muted d-md-none btn-icon btn-sm ms-3 " type="button"
              data-bs-toggle="offcanvas" data-bs-target="#offcanvasAccount" aria-controls="offcanvasAccount">
              <i class="bi bi-text-indent-left fs-3"></i>
            </button>
          </div>
        </div>
        <!-- col -->
        {{> user-account}}
        <div class="col-lg-9 col-md-8 col-12">
          <div class="py-6 p-md-6 p-lg-10">
              <!-- heading -->
            <div class="d-flex justify-content-between mb-6 align-items-center">
              <h2 class="mb-0">Wallet</h2>
              {{#each userWallet}}
                <a href="#" class="btn btn-outline-primary">₹ {{wallet}}.00</a>
                {{/each}}
            </div>
            <ul class="list-group list-group-flush">
              <!-- List group item -->
              {{#each walletTransaction}}
                <li class="list-group-item py-5 px-0">
                    <div class="d-flex justify-content-between">
                    <div class="align-middle border-top-0">
                      #{{orderId}}
                    </div>
                    <div class="align-center border-top-0">
                      {{date}}
                    </div>
                    <div class="col-md-2">
                        <a href="#" class="btn btn-outline-gray-400 text-muted btn-sm">₹ {{total_amount}}</a>
                    </div>
                    </div>
                </li>
              {{/each}}
            </ul>
          </div>
        </div>
      </div>
    </div>
  </section>
</main>